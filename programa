          },
          "metadata": {}
        }
      ],
      "source": [
        "import copy\n",
        "import random\n",
        "import collections\n",
        "import matplotlib.pyplot as plt\n",
        "\n",
        "# -----------------------------\n",
        "# Dados do problema\n",
        "# -----------------------------\n",
        "NUM_VEICULOS = 2\n",
        "TEMPO_MAX = 45\n",
        "\n",
        "dist = [\n",
        " [0, 2.8, 14.2, 12.4, 22.5, 26.5, 39, 11, 6.9, 32.2],\n",
        " [2.8, 0, 12.9, 15, 25.1, 25.2, 37.6, 13.6, 12.1, 30.9],\n",
        " [14.2, 12.9, 0, 12.4, 21.1, 33.5, 13.5, 21.2, 26.7, 9.1],\n",
        " [12.4, 15, 12.4, 0, 1.4, 13.3, 24.5, 16.4, 17.5, 14],\n",
        " [22.5, 25.1, 21.1, 1.4, 0, 17.9, 7, 38.7, 39.1, 26.5],\n",
        " [26.5, 25.2, 33.5, 13.3, 17.9, 0, 27.8, 28.9, 28.2, 39],\n",
        " [39, 37.6, 13.5, 24.5, 7, 27.8, 0, 15.3, 36.7, 26.5],\n",
        " [11, 13.6, 21.2, 16.4, 38.7, 28.9, 15.3, 0, 11.8, 32.9],\n",
        " [6.9, 12.1, 26.7, 17.5, 39.1, 28.2, 36.7, 11.8, 0, 10.3],\n",
        " [32.2, 30.9, 9.1, 14, 26.5, 39, 26.5, 32.9, 10.3, 0]\n",
        "]\n",
        "\n",
        "# -----------------------------\n",
        "# Funções auxiliares\n",
        "# -----------------------------\n",
        "def custo_rota(rota):\n",
        "    return sum(dist[rota[i]][rota[i+1]] for i in range(len(rota)-1))\n",
        "\n",
        "def solucao_viavel(rotas):\n",
        "    return all(custo_rota(r) <= TEMPO_MAX for r in rotas)\n",
        "\n",
        "# -----------------------------\n",
        "# Solução inicial\n",
        "# -----------------------------\n",
        "def solucao_inicial():\n",
        "    clientes = list(range(10))\n",
        "    random.shuffle(clientes)\n",
        "    meio = len(clientes)//2\n",
        "    return [clientes[:meio], clientes[meio:]]\n",
        "\n",
        "# -----------------------------\n",
        "# Busca Tabu\n",
        "# -----------------------------\n",
        "def busca_tabu(iter_max=100):\n",
        "    melhor = solucao_inicial()\n",
        "    melhor_custo = sum(custo_rota(r) for r in melhor)\n",
        "    atual = copy.deepcopy(melhor)\n",
        "    tabu = collections.deque(maxlen=10)\n",
        "    historico = []\n",
        "\n",
        "    for _ in range(iter_max):\n",
        "        vizinhos = []\n",
        "        for v in range(NUM_VEICULOS):\n",
        "            for i in range(len(atual[v])-1):\n",
        "                nova = copy.deepcopy(atual)\n",
        "                nova[v][i], nova[v][i+1] = nova[v][i+1], nova[v][i]\n",
        "                if solucao_viavel(nova) and nova not in tabu:\n",
        "                    vizinhos.append(nova)\n",
        "\n",
        "        if not vizinhos:\n",
        "            break\n",
        "\n",
        "        atual = min(vizinhos, key=lambda r: sum(custo_rota(rt) for rt in r))\n",
        "        tabu.append(atual)\n",
        "        custo_atual = sum(custo_rota(r) for r in atual)\n",
        "        historico.append(custo_atual)\n",
        "\n",
        "        if custo_atual < melhor_custo:\n",
        "            melhor = copy.deepcopy(atual)\n",
        "            melhor_custo = custo_atual\n",
        "\n",
        "    return melhor, historico\n",
        "\n",
        "# -----------------------------\n",
        "# Execução\n",
        "# -----------------------------\n",
        "melhor, hist = busca_tabu()\n",
        "\n",
        "for i, r in enumerate(melhor):\n",
        "    print(f\"Ônibus {i+1}: {r} | Tempo = {custo_rota(r):.2f} min\")\n",
        "\n",
        "plt.plot(hist)\n",
        "plt.xlabel(\"Iteração\")\n",
        "plt.ylabel(\"Custo\")\n",
        "plt.show()\n"
      ]
    }
  ]
}
